	name = Brant Young
	email = anatman.xx@gmail.com

[core]
	editor = /usr/bin/vim
	excludesfile = /Users/flying_mmr/.gitignore_global

[push]
	default = simple

[color]
	status = auto
	diff = auto
	branch = auto
	interactive = auto

[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 

[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
